{
  "name": "express-ts-template",
  "version": "0.1.0",
  "private": true,
  "description": "express ts template",
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/supertest": "^2.0.6",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.0.3",
    "supertest": "^3.3.0"
  },
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "build": "npm run lint && tsc",
    "test": "jest --forceExit",
    "lint": "tslint -c tslint.json -p tsconfig.json --type-check"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.*\\.test\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shiopon01/express-ts-template.git"
  },
  "author": "shiopon01 <shio.0323@gmail.com> (http://shiopon.net)",
  "license": "MIT"
}
